@model temporary.Models.DeactivateUserModel

@{
    ViewBag.Title = "ActivateAUser";
}

<head>
    <link rel="stylesheet" type="text/css" href="~/styling.css">
</head>

<h2>Activate A User</h2>
<br><br>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        <label for="Username">Select Username</label> &nbsp;&nbsp;
        @Html.DropDownListFor(m => m.SelectedUserName, Model.Usernames
                , "Select")

        <br><br>

        <input type="submit" value="Activate" class="btn btn-default" />
    </div>
    <br>
}

<div>
   <h5> @Html.ActionLink("Back to Admin Page", "AdminPage")</h5>
</div>